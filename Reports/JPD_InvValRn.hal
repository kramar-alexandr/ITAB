          MatRowGet(PUr,IHr.Row,PUrw);
          if (IHr.ArtCode==PUrw.ArtCode) then begin
            if (nonblank(RepSpec.f5)) then begin
              if (PUr.VECode==RepSpec.f5) then begin
                res = true;
              end;
            end else begin
              res = true;
            end;
            if (nonblank(RepSpec.f6) or nonblank(RepSpec.f7) and res) then begin
