              CountRecordData(IHr,RepSpec,v_itemreccode,v_itemrecdate,v_itemrecqty,v_itemrecprice,v_itemlen,v_itemlocqty,
                              v_itemloc,v_itemloccostprice,v_itemloccostsum,v_itemlocs);
