  rw = rw + 1;
  OutVal(acols[rw],0,totalq,M45Val,true);
  if (RepSpec.flags[3]!=0) then begin
    rw = rw + 1;
    OutVal(acols[rw],0,totalq2,M45Val,true);
  end;
  rw = rw + 1;
  OutString(acols[rw],0,"",true);
  rw = rw + 1;
  OutVal(acols[rw],0,totalsum,M45Val,true);
  EndFormat;

  return;
end;

